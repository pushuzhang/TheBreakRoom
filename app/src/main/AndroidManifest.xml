<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thebreakroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TheBreakRoom">
        <activity android:name=".CreateSuccess"></activity>
        <activity android:name=".LeaveRoom" />
        <activity android:name=".NewRoom" />
        <activity android:name=".ChatRoom" />
        <activity android:name=".MakeRoom" />
        <activity android:name=".JoinRoom" />
        <activity android:name=".Welcome" />
        <activity android:name=".HomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>